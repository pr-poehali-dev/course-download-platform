{
  "tests": [
    {
      "name": "Test OPTIONS request",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200,
      "expectedHeaders": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "name": "Test missing fileData",
      "method": "POST",
      "path": "/",
      "body": {
        "fileName": "test.pdf"
      },
      "expectedStatus": 400,
      "expectedBody": {
        "error": "fileData is required"
      }
    }
  ]
}
